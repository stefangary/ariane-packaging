&MESH					 ! Model grid data specifics
  dir_mesh     = './', 
  fn_mesh      = 'mesh_mask.nc',
  nc_var_xx_tt = 'glamt', 
  nc_var_xx_uu = 'glamu',  
  nc_var_yy_tt = 'gphit', 
  nc_var_yy_vv = 'gphiv', 
  nc_var_zz_ww = 'gdepw_0',
  nc_var_e2u   = 'e2u', 
  nc_var_e1v   = 'e1v', 
  nc_var_e1t   = 'e1t', 
  nc_var_e2t   = 'e2t', 
  nc_var_e3t   = 'e3t',
  nc_var_tmask = 'tmask',
  nc_mask_val  = 0.,
/
